<h1><%= @car.modelo%></h1>
<h2><%= @car.marca%></h2>
<h3>CAR ID <%= @car.id %></h3>
<h3>Transmision <%= @car.transmision %></h3>
<% @car.images.each do |car_image| %>
  <%= image_tag(car_image, size: '200') %>
<% end %>

<%= link_to 'Edit', edit_car_path(@car) if Current.user&.admin %>
<%= link_to 'Delete', car_path(@car), data:{
  turbo_method: :delete,
  turbo_confirm: t('common.confirm')
} if Current.user&.admin %>
<%= link_to 'Reservar', new_reservation_path(car_id: @car.id) %>


